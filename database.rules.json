{
  "rules": {
    ".read": true,
    ".write": false,
    "tourneySeries": {
      "tourneys": {
        ".write": true
      },
      "players": {
        ".write": "auth != null"
      },
      "playerResults": {
        ".write": "auth != null"
      }
    },
    "elo": {
      ".write": "auth != null",
      "rankedmatches": {
        ".indexOn": [
          "diff",
          "source"
        ]
      },
      "playersV2": {
        ".indexOn": "show"
      }
    },
    "players": {
      ".write": "auth != null",
      ".indexOn": "clubPlayer"
    }
  }
}
